{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch with Dapr",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/server/main.go",
      "env": {
        "DAPR_HTTP_PORT": "3508",
        "DAPR_GRPC_PORT": "50008"
      },
      "preLaunchTask": "dapr-debug",
      "postDebugTask": "dapr-down"
    },
    {
      "name": "Launch without Dapr",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/server/main.go"
    }
  ]
}
